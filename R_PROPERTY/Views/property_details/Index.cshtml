@model IEnumerable<R_PROPERTY.Models.property_details>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.property_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.property_type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.property_address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.property_value)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.property_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.property_type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.property_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.property_value)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.property_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.property_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.property_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
