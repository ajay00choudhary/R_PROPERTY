@model IEnumerable<R_PROPERTY.Models.inquiry>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.user_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.property_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.user_massege)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dateTime_inquiry)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.user_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.property_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.user_massege)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateTime_inquiry)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.inquiry_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.inquiry_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.inquiry_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
